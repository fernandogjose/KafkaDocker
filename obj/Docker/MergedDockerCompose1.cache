services:
  kafka.receive:
    build:
      context: C:\MeusProjetos\Estudo\Kafka
      dockerfile: Src/Kafka.Receive/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Kafka.Receive
      target: base
    container_name: Kafka.Receive
    entrypoint: tail -f /dev/null
    environment:
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: kafkareceive:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Kafka.Receive.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    tty: true
    volumes:
    - C:\MeusProjetos\Estudo\Kafka\Src\Kafka.Receive:/app:rw
    - C:\Users\ferna\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\ferna\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\ferna\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'